{
  "name": "synth",
  "version": "0.1.0",
  "description": "A synthesizer built on the Web Audio API",
  "main": "index.js",
  "devEngines": {
    "node": "5.9.0"
  },
  "engines": {
    "node": "5.9.0"
  },
  "scripts": {
    "start": "node server/index.js",
    "start:prod": "cross-env NODE_ENV=production node server/index.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test",
    "build": "cross-env NODE_ENV=production webpack src/index.js --config webpack.config.production.js",
    "preversion": "npm run clean && npm run check",
    "test": "cross-env NODE_ENV=test tape -r ./src/lib/preamble.js test/*.js",
    "check": "npm run lint && npm run test ",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "doc": "esdoc -c esdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zakangelle/synth.git"
  },
  "keywords": [
    "synth",
    "synthesizer",
    "drum",
    "kick",
    "snare",
    "hihat",
    "oscillator",
    "lfo",
    "adsr",
    "envelope",
    "keys",
    "keyboard",
    "filter",
    "mixer",
    "web",
    "audio",
    "api"
  ],
  "author": "Zak Angelle, Drake Champagne",
  "license": "MIT",
  "dependencies": {
    "audio-context": "^0.1.0",
    "bootstrap": "^3.3.6",
    "mousetrap": "^1.5.3",
    "notes-to-frequencies": "^1.0.0",
    "promise-decode-audio-data": "^0.2.0",
    "react": "^0.14.6",
    "react-bootstrap-switch": "^3.4.1",
    "react-dom": "^0.14.6",
    "react-radio": "^2.1.1",
    "react-router": "^2.0.1"
  },
  "devDependencies": {
    "array-includes": "^3.0.1",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.2",
    "babel-plugin-rewire": "1.0.0-beta-3",
    "babel-plugin-syntax-export-extensions": "^6.3.13",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "chai": "^3.5.0",
    "connect-history-api-fallback": "^1.1.0",
    "coveralls": "^2.11.6",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "esdoc": "^0.4.4",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.16.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.5.0",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.10.0",
    "isparta": "^4.0.0",
    "jsdom": "^7.2.2",
    "mocha": "^2.4.5",
    "nodemock": "^0.3.4",
    "nyc": "^5.3.0",
    "path": "^0.12.7",
    "react-addons-test-utils": "^0.14.7",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "tape": "^3.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  }
}
